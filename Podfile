# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'
use_frameworks!

def network_pod
    pod 'Alamofire'
end

def image_pod
    pod 'Kingfisher'
end


target 'FlickerSampleApp' do
    network_pod
    image_pod
end


target 'FlickerSampleAppTests' do
inherit! :search_paths
# Pods for testing
end
